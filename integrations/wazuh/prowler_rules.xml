<group name="local,amazon,prowler,">
    <!-- Filter 1: Only prowler events -->
    <rule id="110001" level="0">
        <decoded_as>prowler</decoded_as>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <!-- Check Result: Pass -->
    <rule id="110002" level="1">
        <if_sid>110001</if_sid>
        <field name="Message">PASS</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <!-- Check Result: Info -->
    <rule id="110003" level="3">
        <if_sid>110001</if_sid>
        <field name="Message">INFO</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <!-- Check Result: Error -->
    <rule id="110004" level="5">
        <if_sid>110001</if_sid>
        <field name="Message">Error</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <!-- Check Result: Fail, Scored -->
    <rule id="110005" level="9">
        <if_sid>110001</if_sid>
        <field name="Message">FAIL</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <!-- Check Result: Fail, Not Scored -->
    <rule id="110006" level="10">
        <if_sid>110005</if_sid>
        <field name="Severity">Medium</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <rule id="110007" level="11">
        <if_sid>110005</if_sid>
        <field name="Severity">High</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>
    <rule id="110008" level="12">
        <if_sid>110005</if_sid>
        <field name="Severity">Critical</field>
        <description>AWS Check Result: $(Control) - Account $(Account Number)</description>
    </rule>

</group>

